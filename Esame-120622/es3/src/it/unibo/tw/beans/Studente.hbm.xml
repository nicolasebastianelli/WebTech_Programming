<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="it.unibo.tw.beans.Studente" table="studenti">
  <id name="id" column="id">
    	<generator class="increment"/>
    </id>
    <property name="matricola" column="matricola" />
    <property name="nome" column="nome"/>
    <property name="cognome" column="cognome"/>
    <property name="sesso" column="sesso"/>
    <property name="data_nascita" column="data_nascita"/>
  </class>
</hibernate-mapping>